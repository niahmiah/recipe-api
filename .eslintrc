{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": "defaults/configurations/walmart/es6-node",
  "rules": {
    "quotes": [2, "single"],
    "strict": [0, "global"],
    "no-magic-numbers": [0],
    "new-cap": [1],
    "max-params": [0],
    "max-statements": [1],
    "max-nested-callbacks": [0],
    "filenames/filenames": [0],
    "prefer-spread": [0]
  }
}
